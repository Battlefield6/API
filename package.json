{
  "name": "battlefield6-api",
  "version": "1.0.4",
  "description": "This is an basic API to work with DICE's Portal-Server (Battlefield 6).",
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "proto",
    "generator.js",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "proto:generate": "node generator.js",
    "proto:watch": "nodemon --watch proto --exec npm run proto:generate",
    "prepare": "npm run proto:generate && npm run build",
    "prepublishOnly": "npm run build"
  },
  "repository": "battlefield6/API",
  "keywords": [
    "dice",
    "battlefield",
    "bf6",
    "grpc",
    "protobuf",
    "portal"
  ],
  "devDependencies": {
    "@types/google-protobuf": "^3.15.12",
    "@types/node": "^24.7.2",
    "dotenv": "^16.4.7",
    "google-protobuf": "^3.21.4",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "ts-proto": "^2.8.1",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.9.0",
    "grpc-web": "^2.0.2",
    "long": "^5.3.2"
  }
}
